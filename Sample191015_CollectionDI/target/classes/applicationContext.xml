<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="collectionTest" class="com.ofweek.CollectionTest">
        <property name="myArray">
            <array>
                <value>刘备</value>
                <value>关羽</value>
                <value>张飞</value>
            </array>
        </property>

        <property name="myList">
            <list>
                <value>魏</value>
                <value>蜀</value>
                <value>吴</value>
            </list>
        </property>

        <property name="mySet">
            <set>
                <value>联盟</value>
                <value>部落</value>
            </set>
        </property>

        <property name="myMap">
            <map>
                <entry key="China" value="中国"></entry>
                <entry>
                    <key><value>Japan</value></key>
                    <value>日本</value>
                </entry>
            </map>
        </property>

        <property name="myProps">
            <props>
                <prop key="Google">谷歌</prop>
                <prop key="Apple">苹果</prop>
            </props>
        </property>
    </bean>

</beans>